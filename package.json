{
  "name": "app-alpha",
  "version": "1.0.0",
  "description": "Some App",
  "keywords": ["typescript", "javascript"],
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --open",
    "build:html": "parcel build index.html",
    "build": "use-cross-env use-run-all clean build:*",
    "build:node": "use-cross-env use-bili -c bili.config.ts --target node --format cjs --file-name index.js",
    "build:node-mjs": "use-cross-env use-bili -c bili.config.ts --target node --format esm --file-name index.mjs",
    "clean": "use-cross-env use-rimraf .rpt2_cache dist *.*.log",
    "fix": "use-cross-env use-prettier --write '**/*.{ts,json,md,js}'",
    "lint": "use-cross-env use-eslint --fix 'src/**/*.ts'",
    "prepublishOnly": "rushx build",
    "test": "use-cross-env use-jest --detectOpenHandles",
    "test:snapshots": "use-cross-env use-jest --detectOpenHandles -u"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.14.0",
    "lit": "2.0.0-rc.2",
    "parcel-bundler": "^1.6.0"
  },
  "devDependencies": {
    "@renoirb/conventions-use-bili": "^1.1.1",
    "@renoirb/conventions-use-eslint": "^1.1.1",
    "@renoirb/conventions-use-jest": "^1.1.1",
    "@renoirb/conventions-use-prettier": "^1.1.1",
    "@renoirb/tools-bundling-helpers": "^1.1.1",
    "@rushstack/eslint-config": "^2.4.0",
    "core-js": "^3.15.0",
    "eslint": "^6.7.0",
    "typescript": "^4.2.0"
  }
}
