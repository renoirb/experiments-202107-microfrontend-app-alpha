{
  "name": "microfrontend-app-alpha",
  "version": "1.0.0",
  "private": true,
  "description": "MicroFrontend App Alpha",
  "keywords": [
    "typescript",
    "javascript"
  ],
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --open",
    "build:html": "parcel build index.html",
    "build": "use-cross-env use-run-all clean build:*",
    "lint": "use-cross-env eslint --fix '{**/,}*.{ts,js}'",
    "fix": "use-cross-env use-prettier --write '{**/,}*.{ts,tsx,json,md,js,jsx}'"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.14.0",
    "lit": "^2.0.0-rc",
    "parcel-bundler": "^1.12.0"
  },
  "devDependencies": {
    "@renoirb/conventions-use-prettier": "^1.3.0",
    "@renoirb/tools-bundling-helpers": "^1.2.0",
    "core-js": "^3.15.0",
    "eslint": "^6.7.0",
    "tslib": "^2.3.0",
    "@rushstack/eslint-config": "^2.3.4",
    "typescript": "^4.3.0"
  }
}
